                        "example": false,
                        "description": "This field is used to notate Microsoft managed Role Scope tags, e.g. the \"default\" tag."
                    }
                }
            },
            "microsoft.graph.entity": {
                "title": "Graph API - Entity",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "Read-only.",
                        "readOnly": true,
                        "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-5][0-9a-f]{3}-[089ab][0-9a-f]{3}-[0-9a-f]{12}$",
                        "example": "9f237e13-9a04-4daf-b3d4-6d2beec3c2bf"
                    }
                }
            },
            "microsoft.graph.windowsAutopilotDeviceIdentity": {
                "title": "Graph API - Windows Autopilot Device Identity",
                "allOf": [
                    {
                        "$ref": "#/components/schemas/microsoft.graph.entity"
                    },
                    {
                        "title": "windowsAutopilotDeviceIdentity",
                        "type": "object",
                        "properties": {
                            "addressableUserName": {
                                "type": "string",
                                "description": "Addressable user name.",
                                "nullable": true,
                                "example": "Riley Johnson"
                            },
                            "azureActiveDirectoryDeviceId": {
                                "type": "string",
                                "description": "AAD Device ID - to be deprecated",
                                "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-5][0-9a-f]{3}-[089ab][0-9a-f]{3}-[0-9a-f]{12}$",
                                "nullable": true,
                                "example": "de30426d-c90d-42d0-8340-0c587733f3c5"
                            },
                            "azureAdDeviceId": {
                                "type": "string",
                                "description": "AAD Device ID",
                                "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-5][0-9a-f]{3}-[089ab][0-9a-f]{3}-[0-9a-f]{12}$",
                                "nullable": true,
                                "example": "de30426d-c90d-42d0-8340-0c587733f3c5"
                            },
                            "deploymentProfileAssignedDateTime": {
                                "pattern": "^[0-9]{4,}-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]([.][0-9]{1,12})?(Z|[+-][0-9][0-9]:[0-9][0-9])$",
                                "type": "string",
                                "description": "Profile set time of the Windows autopilot device.",
                                "format": "date-time",
                                "example": "2022-02-03T17:42:53Z"
                            },
                            "deploymentProfileAssignmentDetailedStatus": {
                                "type": "string",
                                "description": "Profile assignment detailed status of the Windows autopilot device. Possible values are: none, hardwareRequirementsNotMet, surfaceHubProfileNotSupported, holoLensProfileNotSupported, windowsPcProfileNotSupported, surfaceHub2SProfileNotSupported, unknownFutureValue.",
                                "enum": [
                                    "none",
                                    "hardwareRequirementsNotMet",
                                    "surfaceHubProfileNotSupported",
                                    "holoLensProfileNotSupported",
                                    "windowsPcProfileNotSupported",
                                    "surfaceHub2SProfileNotSupported",
                                    "unknownFutureValue"
                                ],
                                "example": "none"
                            },
                            "deploymentProfileAssignmentStatus": {
                                "description": "Profile assignment status of the Windows autopilot device. Possible values are: unknown, assignedInSync, assignedOutOfSync, assignedUnkownSyncState, notAssigned, pending, failed.",
                                "type": "string",
                                "enum": [
                                    "unknown",
                                    "assignedInSync",
                                    "assignedOutOfSync",
                                    "assignedUnkownSyncState",
                                    "notAssigned",
                                    "pending",
                                    "failed"
                                ],
                                "example": "assignedUnkownSyncState"
                            },
                            "displayName": {
                                "type": "string",
                                "description": "Display Name",
                                "nullable": true,
                                "example": "Computer-Name"
                            },
                            "enrollmentState": {
                                "type": "string",
                                "description": "Intune enrollment state of the Windows autopilot device. Possible values are: unknown, enrolled, pendingReset, failed, notContacted.",
                                "enum": [
                                    "unknown",
                                    "enrolled",
                                    "pendingReset",
                                    "failed",
                                    "notContacted",
                                    "blocked"
                                ],
                                "example": "enrolled"
                            },
                            "groupTag": {
                                "type": "string",
                                "description": "Group Tag of the Windows autopilot device.",
                                "nullable": true,
                                "example": "laptops-2202"
                            },
                            "lastContactedDateTime": {
                                "pattern": "^[0-9]{4,}-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]([.][0-9]{1,12})?(Z|[+-][0-9][0-9]:[0-9][0-9])$",
                                "type": "string",
                                "description": "Intune Last Contacted Date Time of the Windows autopilot device.",
                                "format": "date-time",
                                "example": "2022-02-03T17:42:53Z"
                            },
                            "managedDeviceId": {
                                "type": "string",
                                "description": "Managed Device ID",
                                "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-5][0-9a-f]{3}-[089ab][0-9a-f]{3}-[0-9a-f]{12}$",
                                "nullable": true,
                                "example": "0cc2c546-5c22-4745-ae70-976b7594ca38"
                            },
                            "manufacturer": {
                                "type": "string",
                                "description": "Oem manufacturer of the Windows autopilot device.",
                                "nullable": true,
                                "example": "Microsoft Corporation"
                            },
                            "model": {
                                "type": "string",
                                "description": "Model name of the Windows autopilot device.",
                                "nullable": true,
                                "example": "Surface Book 3"
                            },
                            "productKey": {
                                "type": "string",
                                "description": "Product Key of the Windows autopilot device.",
                                "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-5][0-9a-f]{3}-[089ab][0-9a-f]{3}-[0-9a-f]{12}$",
                                "nullable": true,
                                "example": "d43bb9fc-39af-416a-a64f-7800fd8713a9"
                            },
                            "purchaseOrderIdentifier": {
                                "type": "string",
                                "description": "Purchase Order Identifier of the Windows autopilot device.",
                                "nullable": true,
